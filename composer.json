{
    "name": "vikasrinvi/laravel-bug-watcher",
    "description": "to send the mail when error occured",
    "autoload": {
        "psr-4": {
            "Vikasrinvi\\LaravelBugWatcher\\": "scr/",
            "Vikasrinvi\\LaravelBugWatcher\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Vikas Rinvi",
            "email": "vikas.rinvi@cubettech.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Vikasrinvi\\LaravelBugWatcher\\LaravelBugWatcherServiceProvider"
            ]
        }
    },
    "require-dev": {
        "pestphp/pest": "2.x-dev",
        "orchestra/testbench": "dev-develop"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
        
    }
}
